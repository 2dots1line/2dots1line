{
  "name": "@2dots1line/ui-components",
  "version": "0.1.0",
  "description": "Shared UI components for 2dots1line applications",
  "scripts": {
    "build": "pnpm exec tsc -p tsconfig.build.json && pnpm run copy:css",
    "copy:css": "copyfiles \"src/**/*.css\" dist --up 1",
    "dev": "tsc --watch",
    "lint": "eslint src --max-warnings 0",
    "test": "jest"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "ui",
    "components",
    "react",
    "2dots1line"
  ],
  "author": "2dots1line Team",
  "license": "MIT",
  "dependencies": {
    "@2dots1line/shared-types": "workspace:*",
    "@react-three/drei": "^9.105.6",
    "@react-three/fiber": "^8.16.8",
    "clsx": "^2.0.0",
    "lucide-react": "^0.468.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.3.1",
    "three": "^0.164.1",
    "three-mesh-bvh": "^0.7.0"
  },
  "devDependencies": {
    "@storybook/builder-vite": "^9.0.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.2.0",
    "@types/three": "^0.164.1",
    "copyfiles": "^2.4.1",
    "react": "^18.3.1",
    "typescript": "^5.8.3"
  }
}